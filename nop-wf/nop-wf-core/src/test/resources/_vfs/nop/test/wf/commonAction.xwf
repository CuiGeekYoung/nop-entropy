<wf x:extended="true" bizEntityStateProp="statusId"  x:dump="true">

    <diagram/>

    <actions>
        <action id="sh" displayName="动作0">
            <transition bizEntityState="20" >
              <to-end/>
            </transition>
        </action>
        <action id="_reject" displayName="退回" common="true" >
          <when><c:script>true</c:script></when>
            <transition>
                <to-assigned/>
            </transition>
        </action>
    </actions>

    <steps>
        <step id="start" displayName="开始步骤" >
            <assignment  >
                <actor id="1"     type="user"  />
            </assignment>
            <ref-actions>
                <ref-action actionId="sh" />
            </ref-actions>
        </step>
    </steps>

    <start startStepId="start"/>
</wf>